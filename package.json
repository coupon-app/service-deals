{
  "name": "deal-checkout",
  "version": "1.0.0",
  "description": "Deal/Checkout modal for coupon app",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch --coverage --colors",
    "test:coverage": "jest --coverage --colors",
    "start": "nodemon server",
    "build": "webpack -d -w",
    "seed": "cd db/ && npx sequelize db:drop && npx sequelize db:create && npx sequelize db:migrate && npx sequelize db:seed:all || npm run fallback",
    "fallback": "cd db/ && npx sequelize db:create && npx sequelize db:migrate && npx sequelize db:seed:all",
    "jest": {
      "transform": {
        "^.+\\.jsx?$": "babel-jest"
      }
    }
  },
  "author": "benkidder",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "jest-styled-components": "^7.0.0",
    "jquery": "^3.4.1",
    "mysql2": "^2.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sequelize": "^5.21.3",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^25.1.0",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "sequelize-cli": "^5.5.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
